[auth]
username = <user>
password = <pass>

[kicktipp]
pool_slug = <pool>
tippsaison_id = <id>

[openai]
api_key = sk-proj-XXXXXXXXXXXXXXXXXXXXXXXXXXXXX
model = gpt-5-mini
temperature = 1
timeout = 1200
prompt_profile = research
use_web_search = true

[run]
logfile = kicktipp_bot.log
verbose = true
dry_run = false
# proxy = http://user:pass@host:port

[predictions]
force_fill_all_open_games = true
# alle offenen Spiele mÃ¼ssen einen Tipp erhalten

reject_degenerate_draws = true
# blockiere massenhaft 1:1-Ausgaben

max_draw_share = 0.45
# >45% Remis = degenerate -> einmaliger Retry
